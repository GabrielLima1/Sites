<%= simple_form_for @ctrl_qualit, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :cquality_value %>
  <%= f.input :cquality_date %>
  <%= f.input :sub_cat_ctrl_qualit_id %>
  <%= f.input :product_id %>

  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                ctrl_qualits_path, :class => 'btn btn-default' %>
<% end %>
