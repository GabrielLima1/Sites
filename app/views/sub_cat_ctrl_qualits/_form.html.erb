<%= simple_form_for @sub_cat_ctrl_qualit, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :sccquality_name %>
  <%= f.association :cat_ctrl_qualit, label_method: :ccquality_name, include_blank: false %>

  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                sub_cat_ctrl_qualits_path, :class => 'btn btn-default' %>
<% end %>
