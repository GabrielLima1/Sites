<%- model_class = SubCatCtrlQualit -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:sccquality_name) %>:</strong></dt>
  <dd><%= @sub_cat_ctrl_qualit.sccquality_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:cat_ctrl_qualit_id) %>:</strong></dt>
  <dd><%= @sub_cat_ctrl_qualit.cat_ctrl_qualit_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              sub_cat_ctrl_qualits_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_sub_cat_ctrl_qualit_path(@sub_cat_ctrl_qualit), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              sub_cat_ctrl_qualit_path(@sub_cat_ctrl_qualit),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
