<%- model_class = SubCatCtrlQualit -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:sccquality_name) %></th>
      <th><%= model_class.human_attribute_name(:cat_ctrl_qualit_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @sub_cat_ctrl_qualits.each do |sub_cat_ctrl_qualit| %>
      <tr>
        <td><%= link_to sub_cat_ctrl_qualit.id, sub_cat_ctrl_qualit_path(sub_cat_ctrl_qualit) %></td>
        <td><%= sub_cat_ctrl_qualit.sccquality_name %></td>
        <td><%= sub_cat_ctrl_qualit.cat_ctrl_qualit_id %></td>
        <td><%=l sub_cat_ctrl_qualit.created_at %></td>
        <td>
          <%= link_to t('.show', :default => t("helpers.links.show")),
                      sub_cat_ctrl_qualit_path(sub_cat_ctrl_qualit), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_sub_cat_ctrl_qualit_path(sub_cat_ctrl_qualit), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      sub_cat_ctrl_qualit_path(sub_cat_ctrl_qualit),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_sub_cat_ctrl_qualit_path,
            :class => 'btn btn-primary' %>
