<%= simple_form_for @menu, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :menu_name %>
  <%= f.input :menu_status %>

  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                menus_path, :class => 'btn btn-default' %>
<% end %>
