<%- model_class = FacContact -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:fcontact_name) %></th>
      <th><%= model_class.human_attribute_name(:fcontact_mail) %></th>
      <th><%= model_class.human_attribute_name(:fcontact_tel) %></th>
      <th><%= model_class.human_attribute_name(:fcontact_cel) %></th>
      <th><%= model_class.human_attribute_name(:fcontact_hours) %></th>
      <th><%= model_class.human_attribute_name(:fcontact_status) %></th>
      <th><%= model_class.human_attribute_name(:archive_id) %></th>
      <th><%= model_class.human_attribute_name(:end_factor_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @fac_contacts.each do |fac_contact| %>
      <tr>
        <td><%= link_to fac_contact.id, fac_contact_path(fac_contact) %></td>
        <td><%= fac_contact.fcontact_name %></td>
        <td><%= fac_contact.fcontact_mail %></td>
        <td><%= fac_contact.fcontact_tel %></td>
        <td><%= fac_contact.fcontact_cel %></td>
        <td><%= fac_contact.fcontact_hours %></td>
        <td><%= fac_contact.fcontact_status %></td>
        <td><%= fac_contact.archive_id %></td>
        <td><%= fac_contact.end_factor_id %></td>
        <td><%=l fac_contact.created_at %></td>
        <td>
          <%= link_to t('.show', :default => t("helpers.links.show")),
                      fac_contact_path(fac_contact), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_fac_contact_path(fac_contact), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      fac_contact_path(fac_contact),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_fac_contact_path,
            :class => 'btn btn-primary' %>
