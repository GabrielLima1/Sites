<%- model_class = FacContact -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:fcontact_name) %>:</strong></dt>
  <dd><%= @fac_contact.fcontact_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:fcontact_mail) %>:</strong></dt>
  <dd><%= @fac_contact.fcontact_mail %></dd>
  <dt><strong><%= model_class.human_attribute_name(:fcontact_tel) %>:</strong></dt>
  <dd><%= @fac_contact.fcontact_tel %></dd>
  <dt><strong><%= model_class.human_attribute_name(:fcontact_cel) %>:</strong></dt>
  <dd><%= @fac_contact.fcontact_cel %></dd>
  <dt><strong><%= model_class.human_attribute_name(:fcontact_hours) %>:</strong></dt>
  <dd><%= @fac_contact.fcontact_hours %></dd>
  <dt><strong><%= model_class.human_attribute_name(:fcontact_status) %>:</strong></dt>
  <dd><%= @fac_contact.fcontact_status %></dd>
  <dt><strong><%= model_class.human_attribute_name(:archive_id) %>:</strong></dt>
  <dd><%= @fac_contact.archive_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:end_factor_id) %>:</strong></dt>
  <dd><%= @fac_contact.end_factor_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              fac_contacts_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_fac_contact_path(@fac_contact), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              fac_contact_path(@fac_contact),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
