<%= simple_form_for @sub_menu, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :smenu_name %>
  <%= f.input :smenu_status %>
  <%= f.input :menu_id %>

  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                sub_menus_path, :class => 'btn btn-default' %>
<% end %>
