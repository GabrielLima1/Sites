<%= simple_form_for @accessp, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :accessp_name %>

  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                accessps_path, :class => 'btn btn-default' %>
<% end %>
