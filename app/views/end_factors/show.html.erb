<%- model_class = EndFactor -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:efactory_state) %>:</strong></dt>
  <dd><%= @end_factor.efactory_state %></dd>
  <dt><strong><%= model_class.human_attribute_name(:efactory_city) %>:</strong></dt>
  <dd><%= @end_factor.efactory_city %></dd>
  <dt><strong><%= model_class.human_attribute_name(:efactory_burgh) %>:</strong></dt>
  <dd><%= @end_factor.efactory_burgh %></dd>
  <dt><strong><%= model_class.human_attribute_name(:efactory_street) %>:</strong></dt>
  <dd><%= @end_factor.efactory_street %></dd>
  <dt><strong><%= model_class.human_attribute_name(:efactory_zipcode) %>:</strong></dt>
  <dd><%= @end_factor.efactory_zipcode %></dd>
  <dt><strong><%= model_class.human_attribute_name(:efactory_longitude) %>:</strong></dt>
  <dd><%= @end_factor.efactory_longitude %></dd>
  <dt><strong><%= model_class.human_attribute_name(:efactory_latitude) %>:</strong></dt>
  <dd><%= @end_factor.efactory_latitude %></dd>
  <dt><strong><%= model_class.human_attribute_name(:end_factory_num) %>:</strong></dt>
  <dd><%= @end_factor.end_factory_num %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              end_factors_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_end_factor_path(@end_factor), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              end_factor_path(@end_factor),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
