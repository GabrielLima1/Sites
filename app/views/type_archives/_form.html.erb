<%= simple_form_for @type_archive, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :tarchive_name %>

  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                type_archives_path, :class => 'btn btn-default' %>
<% end %>
