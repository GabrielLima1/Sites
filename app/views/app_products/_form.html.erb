<%= simple_form_for @app_product, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :aproduct_status %>
  <%= f.input :product_id %>
  <%= f.input :app_id %>

  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                app_products_path, :class => 'btn btn-default' %>
<% end %>
